(+) : Int -o Int -o Int;;
(MkInt# x) + (MkInt# y) = MkInt# (x +# y);;

(-) : Int -o Int -o Int;;
(MkInt# x) - (MkInt# y) = MkInt# (x -# y);;

fib_fast_r : Int -o Int -> + Int -> + Int;;
fib_fast_r n a b = case !n of
    0 -> a;
    1 -> b;
    m -> fib_fast_r (m - 1) (a + b) a;;

main : Int;;
main = fib_fast_r 10 1 1;;

