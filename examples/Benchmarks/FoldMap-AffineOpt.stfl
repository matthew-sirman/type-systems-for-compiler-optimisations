(+) : Int -o Int -o Int;;
(MkInt# x) + (MkInt# y) = MkInt# (x +# y);;

map : (a ->? b) -> [a] ->? [b];;
map f xs =
    case ?xs of
        [] -> [];
        (x::xs) -> f x :: map f xs;;

foldr : (a ->? b ->? b) -> b ->? [a] ->? b;;
foldr f e xs =
    case ?xs of
        [] -> e;
        (x::xs) -> f x (foldr f e xs);;

getInt : Int -o Int;;
getInt x = x;;

main : Int;;
main = getInt (foldr (\x ->? \y ->? x + y) 5 (map (\x ->? x + 1) (1::2::3::4::5::[])));;

