(+) : Int -o Int -o Int;;
(MkInt# x) + (MkInt# y) = MkInt# (x +# y);;

(-) : Int -o Int -o Int;;
(MkInt# x) - (MkInt# y) = MkInt# (x -# y);;

fib_fast_n : Int -> Int -> Int -> Int;;
fib_fast_n n a b = case n of
    0 -> a;
    1 -> b;
    n -> fib_fast_n (n - 1) (a + b) a;;

main : Int;;
main = fib_fast_n 10 1 1;;

